#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules/@graphql-codegen/cli/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules/@graphql-codegen/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules/@graphql-codegen/cli/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules/@graphql-codegen/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@25.0.3_graphql@16.12.0_typescript@5.9.3/node_modules:/Users/sb/Dev/chirostretch/system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
fi
